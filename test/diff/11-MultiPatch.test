# date:   14 Nov 2004

DISABLED

eachline: s/^patching .*/patching ((FILE))/


# tests tmtest -d test1 test2 | patch

1.test: file = <<-EOF
    # title:   Ensure the Echo Command Works
    # author:  Sam the Tester
    # date:    31 Mar 2004

    echo "test 1 right output"

    ------ STDOUT: exit code 0 - no error
    test 1 wrong output
EOF

2.test: file = <<-EOF
    # title:   Ensure the Echo Command Works
    # author:  Sam the Tester
    # date:    31 Mar 2004

    echo "test 2 right output"

    ------ STDOUT: exit code 0 - no error
    test 2 wrong output
EOF

tmtest -d $(1.test) $(2.test) | (cd `dirname $(1.test)` && patch)
cat $(1.test) $(2.test)

------ STDOUT: exit code 0 - no error
patching ((FILE))
patching ((FILE))
# title:   Ensure the Echo Command Works
# author:  Sam the Tester
# date:    31 Mar 2004

echo "test 1 right output"

------ STDOUT: exit code 0 - no error
test 1 right output
# title:   Ensure the Echo Command Works
# author:  Sam the Tester
# date:    31 Mar 2004

echo "test 2 right output"

------ STDOUT: exit code 0 - no error
test 2 right output
